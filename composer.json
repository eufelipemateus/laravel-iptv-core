{
    "name": "felipemateus/iptv-core",
    "version": "1.0.0",
    "description": "This package contains the core layouts, classes and other elements for the iptv-channels package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Felipe Mateus",
            "email": "eu@felipemateus.com",
            "homepage": "https://eufelipemateus.com",
            "role": "Developer"
        }
    ],
    "keywords":[
        "Laravel",
        "IPTV",
        "IPTVCHannels",
        "Channels",
        "iptv-channels",
        "channels-m3u8",
        "channels-list",
        "iptv-laravel",
        "m3u8-iptv",
        "channel",
        "Playlist",
        "iptv-playlist",
        "m3u8",
        "Core",
        "IPtv-core",
        "Core-channels",
        "laravel-package"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.54"
	},
	"autoload": {
        "psr-4": {
            "FelipeMateus\\IPTVCore\\":"src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FelipeMateus\\IPTVCore\\IPTVProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "suggest": {
        "felipemateus/iptv-customers": "Allows you to control customers, plans assigned to each channel.",
        "felipemateus/iptv-channels": "Allows you to control channels, groups and cdn."

    }
}
